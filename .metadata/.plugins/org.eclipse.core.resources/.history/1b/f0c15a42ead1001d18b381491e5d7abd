
public class Ex05_Operator {
	public static void main(String[] args) {
		int result = 0;
		result = result + 10;
		result+=10; // result + 10을 줄여서 쓴것을 복합대입연산자 라고함.
		System.out.println(result + 10); // 
		System.out.println(result); // 
		System.out.println("결과값 : JDK33.0");
		System.out.println("결과값 : 6JDK");
	}
}
