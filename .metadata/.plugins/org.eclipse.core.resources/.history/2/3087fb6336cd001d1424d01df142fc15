package pack01_Variable;

public class Ex02_Variable {
	public static void main(String[] args) {
		int a; // 이클립스는 오류코드 외에도 메모리를 위해 불필요한 변수는 지우라고 추천(노란색)
		int b;
		b = 30;
		int c = 40;
		System.out.println(b);
		System.out.println(c);
		// 변수 네이밍 룰
		// 반드시 알파벳 소문자로 시작
		// 자바에서 미리 쓰고 있는 예약어 사용불가능
		// 띄어쓰기 불가능
		// 글자수에 제한이 없음
		// 어떤 내용인지 알아 볼 수 있는 단어가 좋음
	}

}
