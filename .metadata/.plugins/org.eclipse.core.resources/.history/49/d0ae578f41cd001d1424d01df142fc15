package pack02_Byte;

public class Ex01_Byte {
	public static void main(String[] args) {
		byte var1;
		var1 = 0;
		byte var2 = 10;
		System.out.println(var1);
		System.out.println(var2);
		// -128 ~ 127 표현가능
		var1 = -129;
		var2 = 127;
		System.out.println(var1);
		System.out.println(var2);
		
		
	}

}
