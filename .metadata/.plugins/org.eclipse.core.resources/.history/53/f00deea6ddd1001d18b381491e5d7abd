
public class Test_01 {
	public static void main(String[] args) {
		int a;
		byte b;
		long c;
		double d;
		float e;
		String f;
		char g;
		int a = 1;
		byte b = 2;
		long c = 3L;
		double d = 4.4;
		float e = 5.5F;
		String f = "AAA";
		char g = 'a';
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
		System.out.println(e);
		System.out.println(f);
		System.out.println(g);
	}
}
