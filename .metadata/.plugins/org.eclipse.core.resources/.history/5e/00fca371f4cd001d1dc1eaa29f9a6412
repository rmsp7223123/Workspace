package test01_Variable;

public class Test {
	public static void main(String[] args) {
		int num1;
		byte num2;
		long num3;
		double num4;
		float num5;
		num1 = 1;
		num2 = 2;
		num3 = 3L;
		num4 = 4.123;
		num5 = 5.123F;
		System.out.println(num1);
		System.out.println(num2);
		System.out.println(num3);
		System.out.println(num4);
		System.out.println(num5);
	}
}
