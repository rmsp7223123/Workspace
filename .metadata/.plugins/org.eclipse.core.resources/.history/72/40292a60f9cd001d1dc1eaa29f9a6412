package pack05_String;

public class Ex01_String {
	public static void main(String[] args) {
		// 자바에서 제일 많이 사용되는 데이터타입
		// int(정수), double(실수), String(문자열)
		// " == 쌍따옴표 == 더블 코테이션
		String str = "asd";
		System.out.println(str);
		String a = "1";
		String b = "2";
		System.out.println(a+b);
	}
}
